FROM python:3.7

RUN pip install torch===1.4.0 torchvision===0.5.0 -f https://download.pytorch.org/whl/torch_stable.html
RUN pip install fastai --upgrade

COPY fast.py /dataset/train50

CMD /usr/bin/git clone https://github.com/abhishek193/dataset.git && cd dataset && cd train50 && python fast.py